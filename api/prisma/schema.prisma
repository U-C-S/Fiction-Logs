generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model profiles {
  id         BigInt    @id @unique @default(autoincrement())
  created_at DateTime? @default(now()) @db.Timestamptz(6)
  name       String    @unique @db.VarChar
  email      String?   @unique
  password   String?
}
